{
  "version": "2017-02-28",
  "operation": "GetItem",
  "key": {
    "id": $util.dynamodb.toDynamoDBJson($ctx.args.id),
  },
  "condition": {
    "expression": "candidateId = :username",
    "expressionValues": {
      ":username": $util.dynamodb.toDynamoDBJson($context.identity.username)
    }
  }
}