{
  "version": "2017-02-28",
  "operation": "DeleteItem",
  "key": {
    "id": $util.dynamodb.toDynamoDBJson($ctx.args.input.id),
  },
  "condition": {
    "expression": "recruiterId = :username",
    "expressionValues": {
      ":username": $util.dynamodb.toDynamoDBJson($ctx.identity.username)
    }
  }
}